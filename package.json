  },
  "homepage": "https://github.com/dandart/fsm#readme",
  "dependencies": {
    "esm": "^3.2.25",
    "snyk": "^1.316.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.4",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-private-methods": "^7.8.3",
    "babel-eslint": "^11.0.0-beta.2",
    "eslint": "^7.0.0-alpha.0"
  },
